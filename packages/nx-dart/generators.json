{
  "$schema": "http://json-schema.org/schema",
  "name": "nx-dart",
  "version": "0.0.1",
  "generators": {
    "preset": {
      "factory": "./src/generators/preset/generator",
      "schema": "./src/generators/preset/schema.json",
      "description": "Creates an empty workspace for developing Dart and Flutter packages."
    },
    "add-package": {
      "factory": "./src/generators/add-package/generator",
      "schema": "./src/generators/add-package/schema.json",
      "description": "Adds an existing Dart package to the workspace."
    }
  }
}
