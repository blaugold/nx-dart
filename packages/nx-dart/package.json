  "description": "A Nx plugin, that adds support for developing Dart and Flutter packages in a Nx workspace",
  "keywords": [
    "Monorepo",
    "Dart",
    "Flutter",
    "Nx",
    "CLI"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/blaugold/nx-dart"
  },
  "homepage": "https://github.com/blaugold/nx-dart/tree/main/packages/nx-dart",
  "bugs": {
    "url": "https://github.com/blaugold/nx-dart/issues"
  },
  "license": "MIT",
  "main": "src/index.js",
  "generators": "./generators.json",
  "executors": "./executors.json",
  "dependencies": {
    "tslib": "^2.3.0",
    "@nx-dart/dart-parser": "^0.0.1"
  },
  "scripts": {
    "prepublishOnly": "cd ../../ && nx build nx-dart"
