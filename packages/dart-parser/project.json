{
  "root": "packages/dart-parser",
  "sourceRoot": "packages/dart-parser/lib",
  "projectType": "library",
  "targets": {
    "pub-get": {
      "executor": "nx:run-commands",
      "outputs": [
        "packages/dart-parser/.packages",
        "packages/dart-parser/pubspec.lock",
        "packages/dart-parser/.dart_tool/package_config.json"
      ],
      "options": {
        "cwd": "packages/dart-parser",
        "command": "dart pub get"
      }
    },
    "build-base": {
      "executor": "nx:run-commands",
      "dependsOn": ["pub-get"],
      "outputs": ["packages/dart-parser/dist"],
      "options": {
        "cwd": "packages/dart-parser",
        "command": "dart ./tool/build.dart"
      }
    },
    "build": {
      "executor": "nx:noop",
      "outputs": []
    }
  }
}
